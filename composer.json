{
    "name": "symbiote/silverstripe-datatransfer",
    "description": "Assists with transferring data between Silverstripe installations",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "export",
        "import"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "symbiote/silverstripe-multivaluefield": "^5.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "sminnee/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\DataTransfer\\": "src/",
            "Symbiote\\DataTransfer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "expose": []
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}